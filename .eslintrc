{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser":true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "indent": "off",
        "linebreak-style": "off",
        "no-trailing-spaces": "off",
        "eol-last": "off",
        "operator-linebreak": "off"
        
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
}